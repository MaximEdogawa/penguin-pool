{
  "name": "kurrentdb-backend",
  "version": "1.0.0",
  "description": "Backend service for KurrentDB integration with NestJS",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "nest start --watch --env-file=.env",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "docker:build": "docker build -t kurrentdb-backend .",
    "docker:run": "docker run -p 3000:3000 kurrentdb-backend",
    "kill": "../scripts/kill-backend.sh"
  },
  "dependencies": {
    "@kurrent/kurrentdb-client": "~1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/swagger": "^7.1.0",
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/websockets": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "kurrentdb",
    "backend",
    "api",
    "eventsourcing",
    "grpc",
    "nestjs",
    "swagger"
  ],
  "author": "Penguin Pool Team",
  "license": "MIT"
}

# React Native Environment Configuration
# Copy this file to .env.local and fill in your actual values
# This file should NEVER be committed to version control

# Wallet Connect Configuration
# Get your project ID from https://cloud.walletconnect.com/
EXPO_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_actual_wallet_connect_project_id_here

# KurrentDB API Keys
# These are required for KurrentDB functionality
# Get your API keys from your KurrentDB dashboard

# Development Environment
EXPO_PUBLIC_KURRENT_DB_DEV_API_KEY=your_dev_api_key_here
EXPO_PUBLIC_KURRENT_DB_DEV_SECRET_KEY=your_dev_secret_key_here

# Test Environment
EXPO_PUBLIC_KURRENT_DB_TEST_API_KEY=your_test_api_key_here
EXPO_PUBLIC_KURRENT_DB_TEST_SECRET_KEY=your_test_secret_key_here

# Staging Environment
EXPO_PUBLIC_KURRENT_DB_STAGING_API_KEY=your_staging_api_key_here
EXPO_PUBLIC_KURRENT_DB_STAGING_SECRET_KEY=your_staging_secret_key_here

# Production Environment
EXPO_PUBLIC_KURRENT_DB_MAINNET_API_KEY=your_mainnet_api_key_here
EXPO_PUBLIC_KURRENT_DB_MAINNET_SECRET_KEY=your_mainnet_secret_key_here

# Debug Configuration
# Enable debugging in production (set to 'true' to enable)
EXPO_PUBLIC_ENABLE_DEBUG=false
EXPO_PUBLIC_TANSTACK_DEBUG=false
